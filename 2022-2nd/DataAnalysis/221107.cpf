/Link>\n            <Link href=\"\"/>\n            <Link href=\"\"/>\n            <Link href=\"http://support.sas.com/training/studio\">SAS 자습서</Link>\n        </Links>\n    </Registration>\n    <Metadata>\n        <DataSources>\n            <DataSource name=\"dataset\" where=\"true\">\n                <Roles>\n                    <Role exclude=\"columnVariable,strataVariable,freqVariable\" maxVars=\"0\" minVars=\"0\" name=\"rowVariable\" order=\"true\" type=\"A\">행 변수:</Role>\n                    <Role exclude=\"rowVariable,strataVariable,freqVariable\" maxVars=\"0\" minVars=\"0\" name=\"columnVariable\" order=\"true\" type=\"A\">칼럼 변수:</Role>\n                    <Role exclude=\"rowVariable,columnVariable,freqVariable\" maxVars=\"0\" minVars=\"0\" name=\"strataVariable\" order=\"true\" type=\"A\">층화변수:</Role>\n                    <Role exclude=\"rowVariable,columnVariable,strataVariable\" maxVars=\"1\" minVars=\"0\" name=\"freqVariable\" type=\"N\">빈도 수:</Role>\n                </Roles>\n            </DataSource>\n        </DataSources>\n        <Options>\n            <Option inputType=\"string\" name=\"dataTab\">데이터</Option>\n            <Option inputType=\"string\" name=\"optionsTab\">옵션</Option>\n            <Option inputType=\"string\" name=\"dataGroup\">데이터</Option>\n            <Option inputType=\"string\" name=\"rolesGroup\">역할</Option>\n            <Option inputType=\"string\" name=\"addRolesGroup\">추가 역할</Option>\n            <Option inputType=\"string\" name=\"statisticsGroup\">통계량</Option>\n            <Option defaultValue=\"1\" inputType=\"checkbox\" name=\"chiSquareStatistics\">카이제곱 통계량</Option>\n            <!--\"measures\", \"cmh\", \"agree\", \"relrisk\", \"fisher\" options have no effect on one-way tables. -->\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"measureAssociation\">연관성 측도</Option>\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"CMHStatistics\">Cochran-Mantel-Haenszel 통계량</Option>\n            <!--\"agree\" statistics are computed only for square tables. -->\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"measureAgreement\">합치도 측도(정방 테이블 해당)</Option>\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"relRisk\">오즈비 및 상대 리스크(2x2 테이블 해당)</Option>\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"riskDiff\">이항비 및 리스크 차이(2x2 테이블 해당)</Option>\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"exactStats\">정확 통계량</Option>\n            <Option inputType=\"string\" name=\"detailsGroup\">상세 정보</Option>\n            <Option defaultValue=\"nomissing\" inputType=\"combobox\" name=\"missingTreat\">결측값 처리:</Option>\n            <Option inputType=\"string\" name=\"nomissing\">결측값 제외</Option>\n            <Option inputType=\"string\" name=\"missprint\">결측값 빈도 표시</Option>\n            <Option inputType=\"string\" name=\"missing\">계산에 결측값 포함</Option>\n            <Option inputType=\"string\" name=\"exactTestGroup\">정확 검정</Option>\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"exactTest\">Fisher의 정확 검정</Option>\n            <Option inputType=\"string\" name=\"exactTestNote\">주: 일부 큰 문제에서 정확 검정의 계산은 많은 시간과 메모리가 소요될 수 있습니다.</Option>\n            <Option inputType=\"string\" name=\"tableDisplayGroup\">빈도 테이블</Option>\n            <Option inputType=\"string\" name=\"frequenciesGroup\">빈도</Option>\n            <!--\"nofreq\" option has no effect on one-way tables. -->\n            <Option defaultValue=\"1\" inputType=\"checkbox\" name=\"observed\">관측</Option>\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"expected\">기대</Option>\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"deviation\">편차</Option>\n            <Option inputType=\"string\" name=\"percentagesGroup\">백분율</Option>\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"cell\">셀</Option>\n            <!--\"norow\" and \"nocol\" options have no effect on one-way tables. -->\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"row\">행</Option>\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"column\">칼럼</Option>\n            <Option inputType=\"string\" name=\"cumulativeGroup\">누적</Option>\n            <!--\"cumcol\" option has no effect on one-way tables. -->\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"cumColumn\">칼럼 백분율</Option>\n            <!--\"nocum\" option has no effect on crosstabulation tables. -->\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"cumFreqPer\">빈도 및 백분율</Option>\n            <Option inputType=\"string\" name=\"chi2Group\">카이제곱</Option>\n            <!--\"cellchi2\" option has no effect on one-way tables. It doesn't depend on \"chiSquareStatistics\" option. -->\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"cellChi2\">카이제곱 통계량에 대한 셀 분포</Option>\n            <!--\"statsOnly\" option actually turns on/off table display. Removed for this release. Add it back next release? -->\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"statsOnly\">통계량만 표시</Option>\n            <Option inputType=\"string\" name=\"plotsGroup\">도표</Option>\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"suppressPlots\">도표 숨김</Option>\n            <Option defaultValue=\"0\" inputType=\"checkbox\" name=\"freqPlot\">빈도 도표</Option>\n            <!--To produce an agreement plot, you must also specify the AGREE option. -->\n            <!--\"cumfreqplot\" is available for one-way frequency tables only. -->\n            <!--\"deviationplot\" option is available for one-way frequency tables only and requires the \"chisq\" option checked. -->\n            <!--\"mosaicplot\" is available for multiway tables only. -->\n            <!--\"relRiskplot\" option is available for multiway 2x2 tables only and requires the \"measures\" or \"relrisk\" option checked. -->\n            <!--\"scale\" option applies to the FREQPLOT and the CUMFREQPLOT. -->\n            <Option defaultValue=\"freqScale\" indent=\"2\" inputType=\"combobox\" name=\"scale\">빈도 척도:</Option>\n            <Option inputType=\"string\" name=\"freqScale\">척도화되지 않은 빈도</Option>\n            <Option inputType=\"string\" name=\"logScale\">로그(밑 10) 빈도</Option>\n            <Option inputType=\"string\" name=\"percentScale\">백분율(상관 빈도)</Option>\n            <Option inputType=\"string\" name=\"sqrtScale\">빈도 제곱근</Option>\n        </Options>\n    </Metadata>\n    <UI>\n        <Container option=\"dataTab\">\n            <Group open=\"true\" option=\"dataGroup\">\n                <DataItem data=\"dataset\"/>\n            </Group>\n            <Group open=\"true\" option=\"rolesGroup\">\n                <RoleItem role=\"rowVariable\"/>\n                <RoleItem role=\"columnVariable\"/>\n                <RoleItem role=\"strataVariable\"/>\n            </Group>\n            <Group open=\"false\" option=\"addRolesGroup\">\n                <RoleItem role=\"freqVariable\"/>\n            </Group>\n        </Container>\n        <Container option=\"optionsTab\">\n            <Group open=\"false\" option=\"plotsGroup\">\n                <OptionItem option=\"suppressPlots\"/>\n            </Group>\n            <Group open=\"true\" option=\"tableDisplayGroup\">\n                <Group open=\"true\" option=\"frequenciesGroup\">\n                    <OptionItem option=\"observed\"/>\n                    <OptionItem option=\"expected\"/>\n                    <OptionItem option=\"deviation\"/>\n                </Group>\n                <Group open=\"true\" option=\"percentagesGroup\">\n                    <OptionItem option=\"cell\"/>\n                    <OptionItem option=\"row\"/>\n                    <OptionItem option=\"column\"/>\n                </Group>\n                <Group open=\"true\" option=\"cumulativeGroup\">\n                    <OptionItem option=\"cumColumn\"/>\n                    <OptionItem option=\"cumFreqPer\"/>\n                </Group>\n                <Group open=\"true\" option=\"chi2Group\">\n                    <OptionItem option=\"cellChi2\"/>\n                </Group>\n                <!--<OptionItem option=\"statsOnly\"/> -->\n            </Group>\n            <Group open=\"true\" option=\"statisticsGroup\">\n                <OptionItem option=\"chiSquareStatistics\"/>\n                <OptionItem option=\"measureAssociation\"/>\n                <OptionItem option=\"CMHStatistics\"/>\n                <OptionItem option=\"measureAgreement\"/>\n                <OptionItem option=\"relRisk\"/>\n                <OptionItem option=\"riskDiff\"/>\n                <Group open=\"false\" option=\"exactTestGroup\">\n                    <OptionItem option=\"exactTest\"/>\n                    <OptionItem option=\"exactTestNote\"/>\n                </Group>\n            </Group>\n            <Group open=\"false\" option=\"detailsGroup\">\n                <OptionChoice option=\"missingTreat\">\n                    <OptionItem option=\"nomissing\"/>\n                    <OptionItem option=\"missprint\"/>\n                    <OptionItem option=\"missing\"/>\n                </OptionChoice>\n            </Group>\n        </Container>\n    </UI>\n    <Dependencies/>\n    <Requirements>\n        <Requirement condition=\"(!($rowVariable.size()==0 &amp;&amp; $columnVariable.size()==0 &amp;&amp; $strataVariable.size()==0))\">\n            <Message nlsKey=\"varSelectionValMsg1Key\">Select at least one row or column variable.</Message>\n        </Requirement>\n        <Requirement condition=\"(!(($rowVariable.size()==0 || $columnVariable.size()==0) &amp;&amp; $strataVariable.size()&gt;0))\">\n            <Message nlsKey=\"varSelectionValMsg2Key\">Both row and column variables are required to use a strata variable.</Message>\n        </Requirement>\n    </Requirements>\n    <CodeTemplate><![CDATA[\nods noproctitle;\nproc freq data=$dataset;\n   #if ($dataset.getWhereClause() != '')\n   where $dataset.getWhereClause();\n   #end\n\n## full combination of row, column, and strata variables.\n#if($rowVariable.size()>=1 || $columnVariable.size()>=1)\n    tables #if($strataVariable.size()>=1) (#foreach($item in $strataVariable) $item #end)\n#*    *##if($rowVariable.size()>=1) *(#foreach($item in $rowVariable) $item #end)\n#*            *##if($columnVariable.size()>=1) *(#foreach($item in $columnVariable) $item #end) #end\n#*    *##else\n#*            *##if($columnVariable.size()>=1) *(#foreach($item in $columnVariable) $item #end) #end\n#*    *##end\n#else\n#*    *##if($rowVariable.size()>=1) (#foreach($item in $rowVariable) $item #end)\n#*            *##if($columnVariable.size()>=1) *(#foreach($item in $columnVariable) $item #end) #end\n#*    *##else\n#*            *##if($columnVariable.size()>=1) (#foreach($item in $columnVariable) $item #end) #end\n#*    *##end\n#end\n    / #if($chiSquareStatistics==\"1\") chisq #end #if($measureAssociation==\"1\") measures #end #if($CMHStatistics==\"1\") cmh #end\n    #if($measureAgreement==\"1\") agree #end #if($relRisk==\"1\") relrisk #end #if($riskDiff==\"1\") riskdiff #end #if($exactTest==\"1\") fisher #end\n    #if($missingTreat==\"missprint\") missprint #elseif($missingTreat==\"missing\") missing #end\n    #if(!($observed==\"1\")) nofreq #end #if($expected==\"1\") expected #end #if($deviation==\"1\") deviation #end\n    #if(!($cell==\"1\")) nopercent #end #if(!($row==\"1\")) norow #end #if(!($column==\"1\")) nocol #end\n    #if($cumColumn==\"1\") cumcol #end #if(!($cumFreqPer==\"1\")) nocum #end\n    #if($cellChi2==\"1\") cellchi2 #end #if($statsOnly==\"1\") noprint #end \n    #if($suppressPlots==\"1\") plots=none\n    #else\n        plots(only)=(freqplot  ##freqplot for any table request\n        #if((($rowVariable.size()==1 && $columnVariable.size()==0) || ($rowVariable.size()==0 && $columnVariable.size()==1)) && $strataVariable.size()==0)   ##one-way table only\n            cumfreqplot\n        #end\n        #if((($rowVariable.size()==1 && $columnVariable.size()==0) || ($rowVariable.size()==0 && $columnVariable.size()==1)) && $strataVariable.size()==0 && $chiSquareStatistics==\"1\")  ##one-way table and chisq requested\n            deviationplot\n        #end\n        #if($measureAgreement==\"1\") agreeplot #end\n        #if(($measureAssociation==\"1\" || $relRisk==\"1\") && ($rowVariable.size()>=1 && $columnVariable.size()>=1 && $strataVariable.size()>=1)) relriskplot #end   ##for multiway 2x2 tables only\n            #if($riskDiff==\"1\" && ($rowVariable.size()>=1 && $columnVariable.size()>=1 && $strataVariable.size()>=1)) riskdiffplot #end  ##for multiway 2x2 tables only\n        #if(($rowVariable.size()>=1 && $columnVariable.size()>=1)) mosaicplot #end  ##two-way or multiway table request\n        )\n    #end\n    ;\n#end\n   #if($freqVariable.size()>=1) weight #foreach($item in $freqVariable) $item #end; #end\nrun; \n        \n]]></CodeTemplate>\n<TemplateValues version=\"3.8\"><![CDATA[\n{\"strataVariable\":\"\",\"riskDiff\":\"0\",\"addRolesGroup\":\"추가 역할\",\"frequenciesGroup\":\"빈도\",\"cumFreqPer\":\"0\",\"statsOnly\":\"0\",\"sasOS\":\"Linux LIN X64 3.10.0-1062.12.1.el7.x86_64\",\"dataTab\":\"데이터\",\"observed\":\"1\",\"freqPlot\":\"0\",\"percentScale\":\"백분율(상관 빈도)\",\"chiSquareStatistics\":\"1\",\"expected\":\"0\",\"rowVariable\":\"json:[{\\\"value\\\":\\\"CD_Account\\\",\\\"type\\\":\\\"Char\\\",\\\"length\\\":4,\\\"format\\\":\\\"$4.\\\",\\\"informat\\\":\\\"$4.\\\",\\\"className\\\":\\\"RoleObject\\\"}]\",\"suppressPlots\":\"0\",\"logScale\":\"로그(밑 10) 빈도\",\"measureAssociation\":\"1\",\"freqVariable\":\"\",\"missprint\":\"결측값 빈도 표시\",\"cumulativeGroup\":\"누적\",\"plotsGroup\":\"도표\",\"freqScale\":\"척도화되지 않은 빈도\",\"exactStats\":\"0\",\"exactTestNote\":\"주: 일부 큰 문제에서 정확 검정의 계산은 많은 시간과 메모리가 소요될 수 있습니다.\",\"scale\":\"freqScale\",\"cell\":\"0\",\"nomissing\":\"결측값 제외\",\"percentagesGroup\":\"백분율\",\"sqrtScale\":\"빈도 제곱근\",\"rolesGroup\":\"역할\