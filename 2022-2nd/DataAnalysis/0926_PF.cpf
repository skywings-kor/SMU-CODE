andardized variables cannot be the same.</Message>\n        </Requirement>\n    </Requirements>\n    <CodeTemplate><![CDATA[\nods noproctitle;\n\n#if ($byVariables.size() > 0)\nproc sort data=$dataset out=WORK.SORTTempTableSorted;\n   #if ($dataset.getWhereClause() != '')\n   where $dataset.getWhereClause();\n   #end\n    by #foreach( $item in $byVariables ) $item #end ;\nrun;\n#end\n#if ($byVariables.size() > 0)\nproc stdize data=WORK.SORTTempTableSorted\n#else\nproc stdize data=$dataset\n#end\n\n#if ($useCenter == '1') ## Centering only methods\nmethod=#if ($centerMethod == 'meanChoice') mean\n   #elseif ($centerMethod == 'medianChoice') median\n   #end\n#else   ## Standardization methods\nmethod=#if ($stdizeMethod == 'stdDevChoice') std\n   #elseif ($stdizeMethod == 'euclidChoice') euclen\n   #elseif ($stdizeMethod == 'rangeChoice') range\n   #elseif ($stdizeMethod == 'sumChoice') sum\n   #elseif ($stdizeMethod == 'iqrChoice') iqr\n   #elseif ($stdizeMethod == 'abwChoice') abw($bwTuningConstant)\n   #elseif ($stdizeMethod == 'ahuberChoice') ahuber($huberTuningConstant)\n   #elseif ($stdizeMethod == 'awaveChoice') awave($waveTuningConstant)\n   #elseif ($stdizeMethod == 'agkChoice') agk($agkProportion)\n   #elseif ($stdizeMethod == 'madChoice') mad\n   #elseif ($stdizeMethod == 'maxabsChoice') maxabs\n   #elseif ($stdizeMethod == 'spacingChoice') spacing($spacingProportion)\n   #elseif ($stdizeMethod == 'minkowskiChoice') L($lambda)\n   #end\n#end\n\n#if ($pstat == 1) pstat#end\n\n## TODO: implement for centering data only and for standardizing data\n#if ($missingCombo == 'omitChoice') nomiss\n#else #if (!($replaceCombo == 'defaultReplaceChoice')) missing=\n      #if     ($replaceCombo == 'meanLocationChoice')mean\n      #elseif ($replaceCombo == 'medianLocationChoice')median\n      #elseif ($replaceCombo == 'minimumLocationChoice')iqr\n      #elseif ($replaceCombo == 'abwLocationChoice')abw\n      #elseif ($replaceCombo == 'ahuberLocationChoice')ahuber\n      #el