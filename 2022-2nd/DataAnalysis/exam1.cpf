etmax=0.1) xaxisopts=(display=none);\n\t\tlayout gridded / columns=2 border=on autoalign=(topright topleft);\n\t\t%let _lft = halign=left;\n\t\t%let _rgt = halign=right;\n\t\tentry &_lft \"Mean\";\n\t\tentry &_rgt __mean;\n\t\tentry &_lft \"Std Dev\";\n\t\tentry &_rgt __std;\n\t\tentry &_lft \"Min\";\n\t\tentry &_rgt __min;\n\t\tentry &_lft \"Max\";\n\t\tentry &_rgt __max;\n\t\tentry &_lft \"N\";\n\t\tentry &_rgt __n;\n\t\tentry &_lft \"Skewness\";\n\t\tentry &_rgt __skewness;\n\t\tentry &_lft \"Kurtosis\";\n\t\tentry &_rgt __kurtosis;\n\t\tendlayout;\n\t\thistogram AVAR /;\n\t\tendlayout;\n\t\tlayout overlay /;\n\t\tBoxPlot Y=AVAR / orient=horizontal;\n\t\tendlayout;\n\t\tendlayout;\n\t\tendgraph;\n\tend;\nrun;\n\n/* 데이터를 서브셋하고 모든 그룹별 히스토그램 상자를 생성하는 매크로 */\n%macro byGroupHistobox(data=, level=, num_level=, byVars=, num_byvars=, avar=);\n\t%do j=1 %to &num_byvars;\n\t\t%let varName&j=%scan(%str(&byVars), &j);\n\t%end;\n\n\t%do i=1 %to &num_level;\n\n\t\t/* 그룹 변수 값 가져오기 */\n\t\tdata _null_;\n\t\t\ti=&i;\n\t\t\tset &level point=i;\n\n\t\t\t%do j=1 %to &num_byvars;\n\t\t\t\tcall symputx(\"x&j\", strip(&&varName&j), 'l');\n\t\t\t%end;\n\t\t\tstop;\n\t\trun;\n\n\t\t/* proc sql where 절 구성 */\n        %let dsid=%sysfunc(open(&data));\n\t\t%let whereClause=;\n\n\t\t%do j=1 %to %eval(&num_byvars-1);\n\t\t\t%let varnum=%sysfunc(varnum(&dsid, &&varName&j));\n\n\t\t\t%if(%sysfunc(vartype(&dsid, &varnum))=C) %then\n\t\t\t\t%let whereClause=&whereClause.&&varName&j.=\"&&x&j\"%str( and );\n\t\t\t%else\n\t\t\t\t%let whereClause=&whereClause.&&varName&j.=&&x&j.%str( and );\n\t\t%end;\n\t\t%let varnum=%sysfunc(varnum(&dsid, &&varName&num_byvars));\n\n\t\t%if(%sysfunc(vartype(&dsid, &varnum))=C) %then\n\t\t\t%let whereClause=&whereClause.&&varName&num_byvars.=\"&&x&num_byvars\";\n\t\t%else\n\t\t\t%let whereClause=&whereClause.&&varName&num_byvars.=&&x&num_byvars;\n\t\t%let rc=%sysfunc(close(&dsid));\n\n\t\t/* 데이터셋 서브셋 */\n\t\tproc sql noprint;\n\t\t\tcreate table WORK.tempData as select * from &data\n            where &whereClause;\n\t\tquit;\n\n\t\t/* 도표 그룹 정보 구성 */\n        %let groupInfo=;\n\n\t\t%do j=1 %to %eval(&num_byvars-1);\n\t\t\t%let groupInfo=&groupInfo.&&varName&j.=&&x&j%str( );\n\t\t%end;\n\t\t%let groupInfo=&groupInfo.&&varName&num_byvars.=&&x&num_byvars;\n\n\t\t/* 인셋 통계량 가져오기 */\n\t\tdata _null_;\n\t\t\ti=&i;\n\t\t\tset WORK.TempOutputDS6221 point=i;\n\t\t\tcall symputx(\"__mean\", strip(&avar._Mean), 'l');\n\t\t\tcall symputx(\"__std\", strip(&avar._StdDev), 'l');\n\t\t\tcall symputx(\"__min\", strip(&avar._Min), 'l');\n\t\t\tcall symputx(\"__max\", strip(&avar._Max), 'l');\n\t\t\tcall symputx(\"__n\", strip(&avar._N), 'l');\n\t\t\tcall symputx(\"__skewness\", strip(&avar._Skew), 'l');\n\t\t\tcall symputx(\"__kurtosis\", strip(&avar._Kurt), 'l');\n\t\t\tstop;\n\t\trun;\n\n\t\t/* 히스토그램/상자 도표 조합 도표 생성 */\n\t\tproc sgrender data=WORK.tempData template=histobox;\n\t\t\tdynamic AVAR=\"&avar\" ByVarInfo=\" (&groupInfo)\" __mean=\"&__mean\" \n\t\t\t\t__std=\"&__std\" __min=\"&__min\" __max=\"&__max\" __n=\"&__n\" \n\t\t\t\t__skewness=\"&__skewness\" __kurtosis=\"&__kurtosis\";\n\t\trun;\n\n\t%end;\n%mend;\n\n/* 출력 데이터가 생성되지 않으면, 인셋 통계량에 대해 지금 생성 */\nproc means data=MYDATA.UNIV_BANK chartype vardef=df noprint;\n\tvar Age Experience Income CCAvg Mortgage Family;\n\toutput out=WORK.TempOutputDS6221 (replace=yes) \n\t\tmean=std=min=max=n=skewness=kurtosis= / autoname;\nrun;\n\n/* 인셋 통계량 가져오기 */\ndata _null_;\n\tset WORK.TempOutputDS6221;\n\tcall symputx(\"__mean\", strip(Age_Mean), 'l');\n\tcall symputx(\"__std\", strip(Age_StdDev), 'l');\n\tcall symputx(\"__min\", strip(Age_Min), 'l');\n\tcall symputx(\"__max\", strip(Age_Max), 'l');\n\tcall symputx(\"__n\", strip(Age_N), 'l');\n\tcall symputx(\"__skewness\", strip(Age_Skew), 'l');\n\tcall symputx(\"__kurtosis\", strip(Age_Kurt), 'l');\n\tstop;\nrun;\n\nproc sgrender data=MYDATA.UNIV_BANK template=histobox;\n\tdynamic AVAR=\"Age\" ByVarInfo=\"\" __mean=\"&__mean\" __std=\"&__std\" __min=\