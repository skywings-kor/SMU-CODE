        </OptionChoice>\n                    <OptionItem option=\"barLabel\"/>\n                    <OptionItem option=\"barRefLine\"/>\n                    <OptionItem option=\"barRefLineValue\"/>\n                    <OptionItem option=\"barRefValue\"/>\n                    <OptionItem option=\"barRefCustom\"/>\n                    <OptionItem option=\"barRefCustomLabel\"/>\n                </Group>\n                <Group open=\"true\" option=\"lineAxis\">\n                    <OptionChoice option=\"displayLineLabel\">\n                        <OptionItem option=\"defaultLabel\"/>\n                        <OptionItem option=\"noLabel\"/>\n                        <OptionItem option=\"statsLabel\"/>\n                        <OptionItem option=\"noStatsLabel\"/>\n                        <OptionItem option=\"customLabel\"/>\n                    </OptionChoice>\n                    <OptionItem option=\"lineLabel\"/>\n                    <OptionItem option=\"lineRefLine\"/>\n                    <OptionItem option=\"lineRefLineValue\"/>\n                    <OptionItem option=\"lineRefValue\"/>\n                    <OptionItem option=\"lineRefCustom\"/>\n                    <OptionItem option=\"lineRefCustomLabel\"/>\n                </Group>\n            </Group>\n            <Group option=\"legend\">\n                <OptionChoice option=\"legendLoc\">\n                    <OptionItem option=\"outside\"/>\n                    <OptionItem option=\"inside\"/>\n                </OptionChoice>\n            </Group>\n            <Group open=\"false\" option=\"title\">\n                <OptionItem option=\"titleString\"/>\n                <OptionItem option=\"titleSize\"/>\n                <OptionItem option=\"footnoteString\"/>\n                <OptionItem option=\"footnoteSize\"/>\n                <OptionItem option=\"includeAsFootnote\"/>\n            </Group>\n            <Group option=\"graphSize\">\n                <OptionChoice option=\"dimType\">\n                    <OptionItem option=\"in\"/>\n                    <OptionItem option=\"cm\"/>\n                    <OptionItem option=\"px\"/>\n                </OptionChoice>\n                <OptionItem option=\"inchWidth\"/>\n                <OptionItem option=\"inchHeight\"/>\n                <OptionItem option=\"cmWidth\"/>\n                <OptionItem option=\"cmHeight\"/>\n                <OptionItem option=\"pixelWidth\"/>\n                <OptionItem option=\"pixelHeight\"/>\n            </Group>\n        </Container>\n    </UI>\n    <Dependencies>\n        <Dependency condition=\"($dataSource.getWhereClause()!='')\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"includeAsFootnote\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"includeAsFootnote\"/>\n        </Dependency>\n        <Dependency condition=\"($barResponse.size()&gt;0)\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"statistics1\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"statistics1\"/>\n            <Target action=\"show\" conditionResult=\"true\" option=\"barStat\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"barStat\"/>\n        </Dependency>\n        <Dependency condition=\"($lineResponse.size()&gt;0)\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"statistics2\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"statistics2\"/>\n            <Target action=\"show\" conditionResult=\"true\" option=\"lineStat\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"lineStat\"/>\n        </Dependency>\n        <Dependency condition=\"($group.size()&gt;0)\">\n            <Target action=\"disable\" conditionResult=\"true\" option=\"useBarColor\"/>\n            <Target action=\"enable\" conditionResult=\"false\" option=\"useBarColor\"/>\n        </Dependency>\n        <Dependency condition=\"($group.size()==0 &amp;&amp; $useBarColor=='1')\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"barColor\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"barColor\"/>\n        </Dependency>\n        <Dependency condition=\"($group.size()&gt;0)\">\n            <Target action=\"disable\" conditionResult=\"true\" option=\"useLineColor\"/>\n            <Target action=\"enable\" conditionResult=\"false\" option=\"useLineColor\"/>\n        </Dependency>\n        <Dependency condition=\"($group.size()==0 &amp;&amp; $useLineColor=='1')\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"lineColor\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"lineColor\"/>\n        </Dependency>\n        <Dependency condition=\"($displayCatLabel=='customLabel')\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"catLabel\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"catLabel\"/>\n        </Dependency>\n        <Dependency condition=\"($catLabelRotation=='1')\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"catTickRotate\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"catTickRotate\"/>\n        </Dependency>\n        <Dependency condition=\"($displayBarLabel=='customLabel')\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"barLabel\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"barLabel\"/>\n        </Dependency>\n        <Dependency condition=\"($displayLineLabel=='customLabel')\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"lineLabel\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"lineLabel\"/>\n        </Dependency>\n        <Dependency condition=\"($catRefLine=='1')\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"catRefLineValue\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"catRefLineValue\"/>\n            <Target action=\"show\" conditionResult=\"true\" option=\"catRefLineOffset\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"catRefLineOffset\"/>\n            <Target action=\"show\" conditionResult=\"true\" option=\"catRefLabel\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"catRefLabel\"/>\n        </Dependency>\n        <Dependency condition=\"($catRefLine=='1' &amp;&amp; $catRefLabel=='catRefCustom')\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"catRefCustomLabel\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"catRefCustomLabel\"/>\n        </Dependency>\n        <Dependency condition=\"($barRefLine=='1')\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"barRefLineValue\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"barRefLineValue\"/>\n            <Target action=\"show\" conditionResult=\"true\" option=\"barRefLabel\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"barRefLabel\"/>\n        </Dependency>\n        <Dependency condition=\"($barRefLine=='1' &amp;&amp; $barRefLabel=='barRefCustom')\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"barRefCustomLabel\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"barRefCustomLabel\"/>\n        </Dependency>\n        <Dependency condition=\"($lineRefLine=='1')\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"lineRefLineValue\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"lineRefLineValue\"/>\n            <Target action=\"show\" conditionResult=\"true\" option=\"lineRefLabel\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"lineRefLabel\"/>\n        </Dependency>\n        <Dependency condition=\"($lineRefLine=='1' &amp;&amp; $lineRefLab